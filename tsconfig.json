{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "*/.ts",
    "*/.tsx",
    "components/*/.js",
    "components/*/.jsx",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}

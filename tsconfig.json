{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "*/.ts",
    "*/.tsx",
    ".expo/types/*/.ts",
    "expo-env.d.ts",
    "components/*/.js",
    "components/*/.jsx",
    ".expo/types/**/*.ts"
  ]
}
